package sist;

public class Ex04 {

	public static void main(String[] args) {
		
		System.out.println("10 + 2"); // 10 + 2
		
		// 숫자 + 숫자 ==> 숫자(+ 의 역할은 덧셈의 역할)
		System.out.println(10 + 2);
		
		// 문자 + 숫자 ==> 문자(+ 의 역할은 연결(결합)의 역할)
		System.out.println("10" + 2);
		
		// 숫자 + 문자 ==> 문자(+ 의 역할은 연결(결합)의 역할)
		System.out.println(10 + "2");
		
		// 문자 + 문자 ==> 문자(+ 의 역할은 연결(결합)의 역할)
		System.out.println("10" + "2");
		
		System.out.println("a >>>" + 10 + 2);

	}

}
