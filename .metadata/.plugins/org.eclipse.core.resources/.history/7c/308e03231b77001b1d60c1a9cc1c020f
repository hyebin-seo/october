package sist;

public class Ex02 {

	public static void main(String[] args) {
		
		System.out.println("프로그램 시작");
		
		int score = Integer.parseInt(args[0]);
		System.out.println("입력받은 점수 : " + score + "점");
		
		if(score >= 90) {
			System.out.println("점수가 90점 이상힙니다.");
			System.out.println("축하합니다! A학점입니다.");
		}
		
		System.out.println("프로그램 종료");

	}

}
