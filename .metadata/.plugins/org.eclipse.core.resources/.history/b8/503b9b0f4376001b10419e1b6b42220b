package sist;

public class Ex03 {

	public static void main(String[] args) {
		
		double dNum = 123.45666;
		
		System.out.println("dNum >>> " + dNum);
		
		float fNum = (float)dNum;
		
		System.out.println("fNum >>> " + fNum);
		
		float fNum1 = 120.4567f;
		
		System.out.println("fNum1 >>> " + fNum1);

	}

}
