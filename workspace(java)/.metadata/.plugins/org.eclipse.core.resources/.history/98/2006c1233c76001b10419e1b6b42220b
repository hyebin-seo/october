package sist;

public class Ex02 {

	public static void main(String[] args) {
		
		//변수 선언 및 변수 초기화
		byte su1 = 127;
		
		System.out.println("su1 >>> "+ su1);
		
		//자료형의 형변환
		int num1 = su1;
		
		System.out.println("num1 >>> "+ num1);

		int num2 = 150;
		
		//자료형의 형변환(명시적 형변환)
		byte su2 = (byte)num2;
	}

}
